@model IEnumerable<DemoWebApp1.Employee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Department)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HourlyPay)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bonusinc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EmployeeType.EmployeeTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Department)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HourlyPay)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Bonusinc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EmployeeType.EmployeeTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmployeeId}) |
            @Html.ActionLink("Details", "Details", new { id=item.EmployeeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmployeeId })
        </td>
    </tr>
}

</table>
